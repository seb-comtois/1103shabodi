from tkinter import *

# Configuration for the window
root = Tk()
root.title("Test Window")
root.geometry("1200x1200")  # Set the window size large enough to fit all placements

# Import image
image_shabodi = PhotoImage(file="shabodi_logo.gif")

# Display the image with .place()
img = Label(root, image=image_shabodi)
img.place(x=100, y=200)  # Set the x, y coordinates

# Run the application
root.mainloop()